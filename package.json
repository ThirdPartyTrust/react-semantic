    "babel-plugin-react-transform": "^1.1.1",
    "babel-cli": "^6.1.18",
    "eslint": "^1.7.0",
    "eslint-plugin-react": "^3.6.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0",
    "css-loader": "latest"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-portal": "latest"
  }
